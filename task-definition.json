{
    "family": "streamlit-app-task",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::242418121446:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "streamlit-app",
            "image": "242418121446.dkr.ecr.us-east-1.amazonaws.com/sample-streamlit-app:1.0",
            "portMappings": [
                {
                    "containerPort": 8501,
                    "hostPort": 8501,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
